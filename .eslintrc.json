{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [ 
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "@babel"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "warn"
  },
  "parser": "@babel/eslint-parser"
}
